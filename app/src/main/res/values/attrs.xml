<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--通用输入框-->
    <declare-styleable name="EditInputView">
        <attr name="inputIcon" format="reference"></attr>
        <attr name="cleanIcon" format="reference"></attr>
        <attr name="hintText" format="string"></attr>
        <attr name="hintTextColor" format="color"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="inputType" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="base_setting">
        <attr name="icon" format="reference" />
        <attr name="title" format="string" />
    </declare-styleable>

    <declare-styleable name="StateView">
        <attr name="gravity" format="enum">
            <enum name="Top" value="1" />
            <enum name="Left" value="2" />
            <enum name="Right" value="3" />
            <enum name="Bottom" value="4" />
            <enum name="Center" value="5" />
        </attr>

    </declare-styleable>
</resources>